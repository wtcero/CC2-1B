rows = 5
for i in range(1, rows + 1):
    for j in range(1, i + 1):
        print(j, end=' ')
    print('')
    
print ('===============')

total = 0
for i in list(range(1, 5)):
    
    total += i
print(total)

num_input = int(input("Enter a number: "))
sum = 0



for i in range(1, num_input + 1):
    sum += i
print(sum)
    
print ('===============')

rows = 5
for i in range(rows, 1, -1):
    for j in range(1, i + 1):
        print(j, end=' ')
    print(" ")